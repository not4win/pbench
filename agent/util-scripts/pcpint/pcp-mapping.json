{
    "rabbitmq":{
        "ident" : "RabbitMQ Queue Information",
        "probe" : "rabbitmq.queue.name",
        "metrics": ["rabbitmq.queue"]
    },
    "openvswitch":{
        "ident" : "Openvswitch metrics information",
        "probe" : "openvswitch.switch.uuid",
        "metrics": ["openvswitch.switch", "openvswitch.port_info", "openvswitch.flow"]
    },
    "virsh-migrate":{
        "ident" : "metrics used by the pcp-libvirt(1) command",
        "probe" : "libvirt.dominfo.name",
        "metrics": ["libvirt.dominfo.memory.current", "libvirt.dominfo.name", "libvirt.dominfo.uuid",
                    "libvirt.dominfo.vcpu.current", "libvirt.domstats.block.rd.reqs", "libvirt.domstats.block.wr.reqs",
                    "libvirt.domstats.cpu.time", "libvirt.domstats.net.rx.bytes", "libvirt.domstats.net.tx.bytes"]
    },
    "disk":{
        "ident" : "summary disk activity (IOPs and bytes for both reads and writes over all disks)",
        "probe" : "disk",
        "metrics": ["disk.all.read", "disk.all.write", "disk.all.total",
                    "disk.all.read_bytes", "disk.all.write_bytes", "disk.all.total_bytes",
                    "disk.all.avactive"]   
    }
}